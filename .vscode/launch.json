    "version": "0.2.0",
    "configurations": [
        {
            "name": "Web .NET Core Launch",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "buildweb",
            "program": "${workspaceFolder}/PreUni.StaffManagement.Web/bin/Debug/net7.0/PreUni.StaffManagement.Web.dll",
            "args": [],
            "cwd": "${workspaceFolder}/PreUni.StaffManagement.Web",
            "stopAtEntry": false,
            "serverReadyAction": {
                "action": "openExternally",
                "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/PreUni.StaffManagement.Web/Views"
            }
        },
        {
            "name": "AdminWeb .NET Core Launch",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "buildadmin",
            "program": "${workspaceFolder}/PreUni.StaffManagement.AdminWeb/bin/Debug/net7.0/PreUni.StaffManagement.AdminWeb.dll",
            "args": [],
            "cwd": "${workspaceFolder}/PreUni.StaffManagement.AdminWeb",
            "stopAtEntry": false,
            "serverReadyAction": {
                "action": "openExternally",
                "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/PreUni.StaffManagement.AdminWeb/Views"
            }
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach"
        }
    ]
}